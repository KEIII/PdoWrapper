{
    "name": "keiii/pdo-wrapper",
    "type": "library",
    "description": "Wrapper of PHP PDO class.",
    "keywords": ["pdo", "database"],
    "homepage": "https://github.com/KEIII",
    "license": "MIT",
    "authors": [
        {
            "name": "KEIII",
            "homepage": "https://github.com/KEIII"
        }
    ],
    "require": {
        "php": ">=5.5|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "phpunit/dbunit": "^1.0|^2.0",
        "friendsofphp/php-cs-fixer": "^1.0"
    },
    "autoload": {
        "psr-4": {
